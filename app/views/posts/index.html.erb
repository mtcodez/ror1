<div class="col-sm-8 blog-main">
<% @post.each do |post| %>
<div class="blog-post">
	<h2 class="blog-post-title"><%= link_to post.title, post_path(post) %></h2>
	<p class="blog-post-meta"><%= post.updated_at %><a href="#"><%= " " + post.author %></a>
	</p>
	<p><%= post.contents %></p>
</div>
<% end %>
</div>